											</div>
											<div class="input-group mb-3  input-success">
												<div class="input-group-prepend">
													<span class="input-group-text">Code
														Url</span>
												</div>
												<input readonly type="text" class="form-control"
													value="<%=item.meta_tag.code_url%>">
											</div>
											<div class="input-group mb-3  input-success">
												<div class="input-group-prepend">
													<span class="input-group-text">Title</span>
												</div>
												<input readonly type="text" class="form-control"
													value="<%=item.meta_tag.Title%>">
											</div>
											<div class="input-group mb-3  input-success">
												<div class="input-group-prepend">
													<span class="input-group-text">Description</span>
												</div>
												<input readonly type="text" class="form-control"
													value="<%=item.meta_tag.Description%>">
											</div>
											<div class="input-group mb-3  input-success">
												<div class="input-group-prepend">
													<span class="input-group-text">Keywords</span>
												</div>
												<input readonly type="text" class="form-control"
													value="<%=item.meta_tag.Keywords%>">
											</div>
											<div class="input-group mb-3  input-success">
												<div class="input-group-prepend">
													<span class="input-group-text">Url
														Image</span>
												</div>
												<input readonly type="text" class="form-control"
													value="<%=item.meta_tag.url_image%>">
											</div>
											<div class="input-group mb-3  input-success">
												<div class="input-group-prepend">
													<span class="input-group-text">Image
														Alt</span>
												</div>
												<input readonly type="text" class="form-control"
													value="<%=item.meta_tag.image_alt%>">
											</div>
											<div class="input-group mb-3  input-success">
												<img style="width: 100%;border-radius:15px;"
													src="<%=item.meta_tag.url_image%>">
											</div>
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
					<div class="modal-footer">
						<button type="button" class="btn btn-danger light" data-dismiss="modal">Đóng</button>
					</div>
				</div>
			</div>
		</div>
		<%})%>
			<%result_social.forEach(function(item){%>
