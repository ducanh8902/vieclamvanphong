                  <button  id="tieptuc2" type="button" class="button" data-dismiss="modal">Đóng</button>
                </div>
              </div>
              <style>
                .wrapper {
display: flex;
width: 100%;
height: 100%;
justify-content: center;
align-items: center;
}

.screen {
display: flex;
flex-flow: column nowrap;
align-items: center;
justify-content: center;
width: 100%;
background-color: #F5F5F5;
box-shadow: 2px 8px 32px rgba(0,0,0,0.50);
}
.clouds{
position: relative;
bottom: -155px;
width: 243px;
height: 172px;
opacity: 0;
animation: clouds 1s .8s ease-in forwards;
}

.plane {
position: relative;
width: 139px;
height: 139px;
opacity: 0;
animation: plane-start .8s .8s ease-out, plane-loop 1.6s 1.6s linear infinite;

}

.message {
color: transparent;
background-image: linear-gradient( to right bottom, #ffa400, #00aefd );
background-clip: text;
-webkit-background-clip: text;
display: inline-block;
font-weight: bold;
margin-top: 50px;
font-family: Rubik, sans-serif;
font-size: 30px;
letter-spacing: 1px;
animation: message .1s 1.2s ease-in-out backwards;
}

.button{
display: inline-block;
position: relative;
line-height: 46px;
text-transform: capitalize;
font-weight: 400;
cursor: pointer;
height: 50px;
margin: 80px;
padding: 0px  100px;
background: #f36969;
box-shadow: 0 4px 8px 0 rgba(0,0,0,0.16);
border-radius: 15px;
font-size: 18px;
color: whitesmoke;
letter-spacing: -0.05px;
text-align: center;
}

@keyframes plane-start{ 
0% { transform: translate(-80px, 130px) ; }
50% { opacity:1 }
100% { transform: translate(0px, 0); opacity:1  }
}

@keyframes plane-loop {
0% { transform: translate(0px, 0); opacity:1  }
50% { transform: translate(-20px, 30px); opacity:1  }
100% { transform: translate(0px, 0); opacity:1  }
}

@keyframes clouds{
from { opacity: 0 }
to { opacity: 1 }
}

@keyframes message{ 
from { opacity: 0 }
to { opacity: 1 }
}
              </style>
            </div>
          </div>
      
        </div>
      </div>
