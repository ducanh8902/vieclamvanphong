    <script>
         var google = '<%=google%>'
         var page = '<%=page%>'
        if(google == "google_adsense"){
            if(page == "1"){
                setTimeout(async function  () {
                    const response = await fetch(`https://tuyendung.me/radom/jobs`)
                    const data = await response.json();
                    window.open(data.short,"_self")
            }, 15000)
